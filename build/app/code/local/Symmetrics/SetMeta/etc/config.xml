<config>
    <modules>
        <Symmetrics_SetMeta>
            <version>0.1.0</version>
        </Symmetrics_SetMeta>
    </modules>
    <adminhtml>
        <layout>
            <updates>
                <setmeta>
                    <file>setmeta.xml</file>
                </setmeta>
            </updates>
        </layout>
    </adminhtml>   
    <global>
        <blocks>
            <adminhtml>
                <rewrite>
                    <catalog_product_edit>Symmetrics_SetMeta_Block_Catalog_Product_Edit</catalog_product_edit>
                </rewrite>
            </adminhtml>
        </blocks>
        <models>
            <setmeta>
                <class>Symmetrics_SetMeta_Model</class>
            </setmeta>
        </models>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <Symmetrics_SetMeta_Admin before="Mage_Adminhtml">Symmetrics_SetMeta_Admin</Symmetrics_SetMeta_Admin>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
</config>
